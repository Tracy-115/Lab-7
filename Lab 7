#include <iostream>
using namespace std;

void reverseArray(string *names){
  for (int i=0; i=3/2 ; i++){
    string temp = *(names + i);
    *(names + i) = *(names + (2 - i));
    *(names + (2 - i)) = temp;
  }
  return names;
}

void displayArray(string *names){
  for (int i=0 ; i<=3 ; i++){
    cout << *(names+1);
  }
  cout << endl;
}

